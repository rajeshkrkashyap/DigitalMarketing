    <script>
        // Function to handle screen size change
        function handleResize() {
            // Get the current width and height of the window
            var screenWidth = window.innerWidth;
            var screenHeight = window.innerHeight;
            // Display the width and height in the console
            if (screenWidth <= 600) {
                $('#txtAnalysis').removeClass("input-lg").addClass("input-sm");
                $('#btnAnalysis').removeClass("btn-lg").addClass("btn-sm");
            } else {
                $('#txtAnalysis').removeClass("input-sm").addClass("input-lg");
                $('#btnAnalysis').removeClass("btn-sm").addClass("btn-lg");
            }
            // Add your custom logic here based on the screen size
            // For example, you can trigger different actions for different screen sizes
        }
        // Add event listener for the resize event
        window.addEventListener('resize', handleResize);
        // Call the handleResize function initially to get the screen size on page load
        handleResize();
    </script>
