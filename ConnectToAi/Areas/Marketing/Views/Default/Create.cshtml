<body>
    <div id="app">

        <div class="app-content">
            @{
                await Html.RenderPartialAsync("~/Areas/Marketing/Views/Shared/_Header.cshtml");
            }
            <div class="main-content" style="margin-left:0px">
                <div class="wrap-content container" id="container">
                    <section id="page-title">
                        <div class="row">
                            <div class="col-sm-8">
                                <h1 class="mainTitle">Create a Project</h1>
                                <span class="mainDescription">
                                    Hit 'Create' to finalize the project setup.
                                </span>
                            </div>

                        </div>
                    </section>
                    <div class="container-fluid container-fullw bg-white">
                        <div class="row">
                            <div class="col-md-12">
                                <h5 class="over-title margin-bottom-15">
                                    Create <span class="text-bold">Project</span>
                                </h5>
                                <p>Some textboxes in this example is required.</p>
                                <form action="#" role="form" class="smart-wizard" id="form">
                                    <div id="wizard" class="swMain">
                                        <ul>
                                            <li>
                                                <a href="#step-1">
                                                    <div class="stepNumber">1</div>
                                                    <span class="stepDesc">
                                                        <small>Project Information</small>
                                                    </span>
                                                </a>
                                            </li>
                                            <li>
                                                <a href="#step-2">
                                                    <div class="stepNumber">2</div>
                                                    <span class="stepDesc">
                                                        <small>Create a Project</small>
                                                    </span>
                                                </a>
                                            </li>
                                            <li>
                                                <a href="#step-3">
                                                    <div class="stepNumber">3</div>
                                                    <span class="stepDesc">
                                                        <small>Complete</small>
                                                    </span>
                                                </a>
                                            </li>
                                        </ul>
                                        <div id="step-1">
                                            <div class="row">
                                                <div class="col-md-5">
                                                    <div class="padding-30">
                                                        <h2 class="StepTitle">
                                                            <i class="ti-face-smile fa-2x text-primary block margin-bottom-10"></i> Enter your project information
                                                        </h2>
                                                    </div>
                                                </div>
                                                <div class="col-md-7">
                                                    <fieldset>
                                                        <legend>Personal Information</legend>
                                                        <div class="row">
                                                            <div class="col-md-12">
                                                                <div class="form-group">
                                                                    <label>
                                                                        Project Name <span class="symbol required"></span>
                                                                    </label>
                                                                    <input type="text" placeholder="Enter your Project Name" class="form-control" name="firstName">
                                                                </div>
                                                            </div>
                                                            <div class="col-md-12">
                                                                <div class="form-group">
                                                                    <label>
                                                                        Project Description <span class="symbol required"></span>
                                                                    </label>
                                                                    <textarea type="text" placeholder="Enter Project Description" class="form-control" name="descriptionName"></textarea>
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <p>
                                                            <a href="javascript:void(0)" data-content="Your project information is not required for unlawful purposes, but only in order to analysis perpuse" data-title="Don't worry!" data-placement="top" data-toggle="popover">Why do you need my Project information?</a>
                                                        </p>
                                                    </fieldset>

                                                    <div class="form-group">
                                                        <button class="btn btn-primary btn-o next-step btn-wide pull-right">
                                                            Next <i class="fa fa-arrow-circle-right"></i>
                                                        </button>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div id="step-2">
                                            <div class="row">
                                                <div class="col-md-5">
                                                    <div class="padding-30">
                                                        <h2 class="StepTitle">
                                                            Create an account <span class="text-large block">to manage everything you do with ClipTwo</span>
                                                        </h2>
                                                        <p>You’ll enjoy personal services and great benefits including:</p>
                                                        <p class="text-small">
                                                            <ul class="no-margin">
                                                                <li>Access to exclusive releases and limited products.</li>
                                                                <li>ng-Clip services, benefits and promotions.</li>
                                                            </ul>
                                                        </p>
                                                    </div>
                                                </div>
                                                <div class="col-md-7">
                                                    <fieldset>
                                                        <legend>Account Credential</legend>
                                                        <div class="form-group">
                                                            <label class="control-label">
                                                                WebSite ULR <span class="symbol required"></span>
                                                            </label>
                                                            <input type="url" placeholder="Enter your's Website ULR'" class="form-control" name="URL">
                                                        </div>

                                                    </fieldset>
                                                    <div class="form-group">
                                                        <button class="btn btn-primary btn-o back-step btn-wide pull-left">
                                                            <i class="fa fa-circle-arrow-left"></i> Back
                                                        </button>
                                                        <button class="btn btn-primary btn-o next-step btn-wide pull-right">
                                                            Next <i class="fa fa-arrow-circle-right"></i>
                                                        </button>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div id="step-3">
                                            <div class="row">
                                                <div class="col-md-12">
                                                    <div class="text-center">
                                                        <h1 class="ti-check block text-primary"></h1>
                                                        <h2 class="StepTitle">Congratulations!</h2>
                                                        <p class="text-large">Your bebsite anlysis is started.</p>
                                                        <a class="btn btn-primary btn-o" id="linkBtnFinish" href="/marketing/dashboard/Index">Click here to see result</a>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </form>
                            </div>
                        </div>
                    </div>
                    
                </div>
            </div>
        </div>
