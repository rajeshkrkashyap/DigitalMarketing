    <script>
        NProgress.configure({
            showSpinner: !1
        }), NProgress.start(), NProgress.set(.4);
        var interval = setInterval(function () {
            NProgress.inc()
        }, 1e3);
        jQuery(document).ready(function () {
            NProgress.done(),
                clearInterval(interval),
                Main.init()
        })
    </script>
    <script src="/assets/js/form-wizard.min.js"></script>
    <script>
        jQuery(document).ready(function () {
            FormWizard.init()
        })
    </script>
  

